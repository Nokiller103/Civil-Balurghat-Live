  <section id="home" class="home-section">
    <div class="hero">
      <div class="content">
        <h1 class="headline">Welcome to Our Site</h1>
        <p class="subheadline">Your success is our priority.</p>
        <a href="/contact" class="cta">Get Started</a>
      </div>
    </div>
  </section>

  <section id="service" class="service-section">
    <div class="container mt-5">
      <h1 class="pb-2 text-center border-bottom">Services</h1>
      <div class="py-5 bg-body-tertiary d-md-none">
        <div class="container">
          <div>
            {{#jobs}}
            <div class="card-1 border">
              <img src="{{image}}" class="card-img-top" alt="{{title}}">
              <div class="card-body-1">
                <h5>{{title}}</h5>
                <p>{{location}}</p>
                <a href="/jobs/{{id}}" class="btn btn-primary">Contact Us</a>
              </div>
            </div>
            {{/jobs}}
          </div>
        </div>
      </div>
      <div class="album py-5 bg-body-tertiary d-none d-md-block">
        <div class="container">

